          body:
            application/json:
              example:
                  {"message":"Resource not found"}
        500:
          description: Returns process failed error
          body:
            application/json:
              example:
                {"message":"Internal Server Error"}    
    put:
      body:
        application/json:
          example: !include request/put-creditcheck-request.json
            
      responses:
        200:
          body:
            application/json:
               example: !include request/put-creditcheck-response.json
        400:
          description: Returns bad request
          body:
            application/json:
              example:
                {"message":"Bad Request"}                
        404:
          description: Returns record not found
          body:
            application/json:
              example:
                  {"message":"Resource not found"}
        500:
          description: Returns process failed error
          body:
            application/json:
              example:
                {"message":"Internal Server Error"}                               
    
    
